<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane prefHeight="145.0" prefWidth="250.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.laboratory.AddPurchaseController">
    <content>
        <VBox alignment="TOP_CENTER" fillWidth="false" prefHeight="151.0" prefWidth="348.0" spacing="10" style="-fx-background-color: white;">
            <children>

                <TextField fx:id="productName" promptText="product Name"  prefWidth="180"/>
                <ChoiceBox fx:id="type" prefWidth="180" value="consomable">
                    <items>
                        <FXCollections fx:factory="observableArrayList">
                            <String fx:value="consomable" />
                            <String fx:value="reactif" />
                        </FXCollections>
                    </items>
                </ChoiceBox>
                <TextField fx:id="montant" promptText="montant" prefWidth="180"/>
                <DatePicker fx:id="date" prefHeight="25.0" prefWidth="180.0" promptText="date" />

            </children>
        </VBox>
    </content>
</DialogPane>
