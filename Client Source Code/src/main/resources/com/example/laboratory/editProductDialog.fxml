<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane prefHeight="145.0" prefWidth="250.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.laboratory.EditProductDialog">
    <header>
        <VBox alignment="TOP_CENTER" fillWidth="false" prefHeight="151.0" prefWidth="348.0" spacing="10" style="-fx-background-color: white;">
            <children>
                <Label layoutX="83.0" layoutY="67.0" style="-fx-font-size: 25; -fx-font-weight: bold;" text="Edit Product" />
                <VBox layoutX="88.0" layoutY="120.0" prefHeight="156.0" prefWidth="191.0" spacing="20.0">
                    <children>
                        <TextField fx:id="productName" prefWidth="180"/>
                        <TextField fx:id="quantity" prefWidth="180" />
                        <ChoiceBox fx:id="type" prefWidth="180" value="consomable">
                            <items>
                                <FXCollections fx:factory="observableArrayList">
                                    <String fx:value="consomable" />
                                    <String fx:value="reactif" />
                                </FXCollections>
                            </items>
                        </ChoiceBox>
                    </children>
                </VBox>
            </children>
        </VBox>
    </header>
</DialogPane>
