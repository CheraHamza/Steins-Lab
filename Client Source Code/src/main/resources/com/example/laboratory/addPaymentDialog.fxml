    <content>
        <VBox alignment="TOP_CENTER" fillWidth="false" prefHeight="151.0" prefWidth="348.0" spacing="10" style="-fx-background-color: white;">
            <children>
                <ChoiceBox fx:id="usernameChoice" prefHeight="25.0" prefWidth="151.0" />

                <TextField fx:id="montant" promptText="montant"  prefWidth="151"/>
